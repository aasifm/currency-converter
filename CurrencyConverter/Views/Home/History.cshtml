@model IEnumerable<CurrencyConverter.Models.CurrencyConversionAudit>

@{
    ViewData["Title"] = "History";
}

<h1 class="display-4">History</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AuditDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserValue)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CurrencyName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CurrencyValue)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ConvertedValue)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AuditDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserValue)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CurrencyName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CurrencyValue)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ConvertedValue) @Html.DisplayFor(modelItem => item.CurrencyName)
            </td>
        </tr>
}
    </tbody>
</table>
